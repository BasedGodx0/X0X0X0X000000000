MODS = {
    [3394884959] = true; -- zulu   
}

ADMIN = {
    [3394885524] = true; -- thy
}

--// Don't mess with anything below this
local function main()
    for i,v in pairs(game.Players:GetChildren()) do
        if table.find(ADMIN,v.UserId) then
		if v.Character then
			v.Character:FindFirstChild("Humanoid").DisplayName = "[ğŸ‘‘]"..v.DisplayName
		end
        elseif table.find(MODS,v.UserId) then
	    if v.Character then
		v.Character:FindFirstChild("Humanoid").DisplayName = "[â˜‘ï¸]"..v.DisplayName
	    end
        else
	    if v.Character then
	          if not v.Character.Torso:FindFirstChild("roblox") then
		        v.Character:FindFirstChild("Humanoid").DisplayName = "[ğŸˆ]"..v.DisplayName
		  end
	    end
	end
    end
end

local success, err = pcall(main)
if err then print(err) end
return MODS, ADMIN
